{
    "name": "genie-fintech/dev-login",
    "description": "Laravel Developer Login",
    "keywords": [
        "gennie-fintech",
        "laravel",
        "developer",
        "login"
    ],
    "homepage": "https://github.com/genie-fintech/dev-login",
    "license": "MIT",
    "authors": [
        {
            "name": "Tint Naing Win",
            "email": "tintnaingwin@geniefintech.com",
            "role": "Developer"
        },
        {
            "name": "Myat Htut Thu",
            "email": "myathtutthu@geniefintech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "GenieFintech\\DevLogin\\": "src",
            "GenieFintech\\DevLogin\\Auth\\": "auth-backend/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GenieFintech\\DevLogin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GenieFintech\\DevLogin\\DevLoginServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
