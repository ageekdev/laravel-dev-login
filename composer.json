{
    "name": "ageekdev/dev-login",
    "description": "Developer Login for Laravel",
    "keywords": [
        "gennie-fintech",
        "laravel",
        "developer",
        "login",
        "developer-login",
        "developer-auth"
    ],
    "homepage": "https://github.com/ageekdev/laravel-dev-login",
    "license": "MIT",
    "authors": [
        {
            "name": "Tint Naing Win",
            "email": "tintnaingwin@geniefintech.com",
            "role": "Developer"
        },
        {
            "name": "Myat Htut Thu",
            "email": "myathtutthu@geniefintech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.10",
        "larastan/larastan": "^2.0",
        "orchestra/testbench": "^8.11|^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "AgeekDev\\DevLogin\\": "src",
            "AgeekDev\\DevLogin\\Auth\\": "auth-backend/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AgeekDev\\DevLogin\\Tests\\": "tests"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse",
        "analyse-clear": "vendor/bin/phpstan clear-result-cache",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AgeekDev\\DevLogin\\DevLoginServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
